<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi = http://www.w3.org/2001/XMLSchema-instance xmlns=http://xmlns.jcp.org/xml/ns/javaee xsi:schemaLocation=http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd id="WebApp_ID" version="4.0">
  
<display-name>com.multiplex</display-name>
  <welcome-file-list>
    <welcome-file>BookSeats</welcome-file>
    <welcome-file>ServletForShowUser</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>booking</servlet-name>
  <servlet-class>com.multiplex.business.BookSeats</servlet-class>
  </servlet>
  <servlet> 
  <servlet-name>showuser </servlet-name>
  <servlet-class>com.multiplex.business.ServletForShowUser</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>booking</servlet-name>
  <url-pattern>/booking</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>showuser</servlet-name>
  <url-pattern>/showuser</url-pattern>
  </servlet-mapping>
  
  
</web-app>
 